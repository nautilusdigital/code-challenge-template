    "/contacts": {
      "get": {
        "tags": ["contacts"],
        "summary": "Get contacts",
        "description": "Get all contacts",
        "parameters": [
          {
            "name": "firstName",
            "in": "query",
            "description": "Contact first name",
            "required": false,
            "schema": {
              "type": "string",
              "example": "John"
            }
          },
          {
            "name": "lastName",
            "in": "query",
            "description": "Contact last name",
            "required": false,
            "schema": {
              "type": "string",
              "example": "Doe"
            }
          },
          {
            "name": "phone",
            "in": "query",
            "description": "Contact phone number",
            "required": false,
            "schema": {
              "type": "string",
              "example": "+17777777777"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string",
                        "format": "uuid"
                      },
                      "category": {
                        "type": "string"
                      },
                      "firstName": {
                        "type": "string"
                      },
                      "lastName": {
                        "type": "string"
                      },
                      "age": {
                        "type": "integer"
                      },
                      "email": {
                        "type": "string"
                      },
                      "phone": {
                        "type": "number"
                      },
                      "city": {
                        "type": "boolean"
                      },
                      "region": {
                        "type": "string"
                      }
                    }
                  }
                },
                "examples": {
                  "Contacts list": {
                    "value": [
                      {
                        "id": "3fa85f64-5717-4562-b3fc-2c963f66afa6",
                        "category": "Family",
                        "firstName": "John",
                        "lastName": "Doe",
                        "age": 30,
                        "email": "john@doe.com",
                        "phone": "+17777777777",
                        "city": "Vancouver",
                        "region": "British Columbia"
                      }
                    ]
                  }
                }
              }
            }
          },
          "500": {
            "description": "Infrastructure error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": [
                    "name",
                    "code"
                  ],
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "code": {
                      "type": "string"
                    },
                    "engine": {
                      "type": "string"
                    },
                    "engineCode": {
                      "type": "string"
                    }
                  }
                },
                "examples": {
                  "Database errors": {
                    "value": {
                      "name": "DatabaseError",
                      "code": "E02000",
                      "engine": "prisma",
                      "engineCode": "Pxxxx"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": ["contacts"],
        "summary": "Create contact",
        "description": "Create a new contact",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "firstName": {
                    "type": "string"
                  },
                  "lastName": {
                    "type": "string"
                  },
                  "category": {
                    "type": "string"
                  },
                  "age": {
                    "type": "integer"
                  },
                  "email": {
                    "type": "string"
                  },
                  "phone": {
                    "type": "string"
                  },
                  "city": {
                    "type": "string"
                  },
                  "regionId": {
                    "type": "string",
                    "format": "uuid"
                  },
                  "regionOther": {
                    "type": "string"
                  }
                }
              },
              "examples": {
                "Created User with regionId": {
                  "value": {
                    "firstName": "John",
                    "lastName": "Doe",
                    "category": "Family",
                    "age": 30,
                    "email": "john@doe.com",
                    "phone": "+17777777777",
                    "city": "Vancouver",
                    "regionId": "3fa85f64-5717-4562-b3fc-2c963f66afa6"
                  }
                },
                "Created User with other region": {
                  "value": {
                    "firstName": "John",
                    "lastName": "Doe",
                    "category": "Family",
                    "age": 30,
                    "email": "john@doe.com",
                    "phone": "+17777777777",
                    "city": "Vancouver",
                    "regionId": "3fa85f64-5717-4562-b3fc-2c963f66afa6",
                    "regionOther": "United States"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string",
                      "format": "uuid"
                    }
                  }
                },
                "examples": {
                  "Contact create": {
                    "value": {
                      "id": "3fa85f64-5717-4562-b3fc-2c963f66afa6"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Client errors",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": ["name", "code"],
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "code": {
                      "type": "string"
                    },
                    "errorFields": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "required": ["path", "message"],
                        "properties": {
                          "path": {
                            "type": "string"
                          },
                          "message": {
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                },
                "examples": {
                  "Invalid user/password": {
                    "value": {
                      "name": "BadRequestError",
                      "code": "E00200"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Infrastructure error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": [
                    "name",
                    "code"
                  ],
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "code": {
                      "type": "string"
                    },
                    "engine": {
                      "type": "string"
                    },
                    "engineCode": {
                      "type": "string"
                    }
                  }
                },
                "examples": {
                  "Database errors": {
                    "value": {
                      "name": "DatabaseError",
                      "code": "E02000",
                      "engine": "prisma",
                      "engineCode": "Pxxxx"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
